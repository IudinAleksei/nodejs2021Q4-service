{
  "name": "rss-rest-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:start": "docker compose up",
    "docker:build": "docker compose up --build",
    "docker:stop": "docker compose down --volumes",
    "prepare": "husky install && npx prisma generate",
    "start:migrate": "prisma migrate dev --preview-feature && prisma db seed && npm run start:dev"
  },
  "dependencies": {
    "@nestjs/common": "8.2.6",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mapped-types": "1.0.1",
    "@nestjs/passport": "8.1.0",
    "@nestjs/platform-express": "8.2.6",
    "@nestjs/platform-fastify": "8.2.6",
    "@prisma/client": "3.8.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "nest-winston": "1.6.2",
    "nestjs-form-data": "1.6.1",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "prisma": "3.8.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.2",
    "winston": "3.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.0",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.6",
    "@types/bcrypt": "5.0.0",
    "@types/jest": "27.4.0",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.12",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "debug": "4.3.3",
    "dirty-chai": "2.0.1",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ],
    "src/**/*.ts": [
      "npm run lint"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}